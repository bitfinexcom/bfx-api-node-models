{
  "name": "bfx-api-node-models",
  "version": "1.2.5",
  "description": "Object models for usage with the Bitfinex node API",
  "engines": {
    "node": ">=8.3.0"
  },
  "main": "lib/index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.ts src/ examples/ test/",
    "test": "npm run lint && npm run types && npm run unit",
    "unit": "NODE_ENV=test nyc mocha --recursive",
    "build": "./node_modules/.bin/babel src/ -d lib/",
    "docs": "rm -rf docs && node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "serve-docs": "./node_modules/.bin/http-server docs",
    "types": "./node_modules/.bin/tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-api-node-models.git"
  },
  "keywords": [
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "author": "Bitfinex",
  "contributors": [
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Ezequiel Wernicke <ezequiel.wernicke@bitfinex.com> (https://www.bitfinex.com)",
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-api-node-models/issues"
  },
  "homepage": "http://bitfinexcom.github.io/bfx-api-node-models/",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "bfx-api-node-rest": "^3.0.3",
    "chai": "^4.2.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "http-server": "^0.12.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bfx-api-node-util": "^1.0.8",
    "bfx-hf-util": "^1.0.6",
    "bluebird": "^3.7.2",
    "crc-32": "^1.2.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.15"
  }
}
